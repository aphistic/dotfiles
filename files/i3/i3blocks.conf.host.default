[volume]
label=VOL
instance=Master
command=/usr/local/libexec/i3blocks/volume
interval=once
signal=10

[ethernet]
label=E
instance=eno1
command=/usr/local/libexec/i3blocks/network
color=#00FF00
interval=10

[disk]
label=R
instance=/
command=/usr/local/libexec/i3blocks/disk
interval=30

[disk]
label=D
instance=/var/lib/docker
command=/usr/local/libexec/i3blocks/disk
interval=30

[disk]
label=H
instance=/home
command=/usr/local/libexec/i3blocks/disk
interval=30
